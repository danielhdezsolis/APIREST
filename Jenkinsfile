pipeline{
    agente any{
        stage('Build'){
            steps{
                echo "Etapa Build no disponible"
            }
            
        }
        stage('Tests'){
            steps{
                echo "Etapa Test no disponible"
            }
            
        }
        stage('Deploy'){
            steps{
                sh "docker compose up"
            }
            
        }
    }
}